<div class="col-lg-12">
	<div class="full no-border">
		<div class="table-responsive">
			<% unless @resp.empty? %>
				<div class="paginate_ajax">
					<table class="table table-th-block table-primary">
						<thead>
							<tr><th style="width: 30px;">No.</th><th>Problem</th><th>Programming Language</th><th>Date of submission</th><th>Status</th></tr>
						</thead>
						<tbody>
							<% i = 1 %>
							<% @resp.first(8).each do |item| %>
							<tr><td><%= i %></td><td><%= item["problem"]["name"] %></td><td><%= item["programmingLanguage"] %></td><td> <%= Time.at(item["creationTimeSeconds"]).to_date %></td><td><span class="label label-<%= color_status(item["verdict"]) %>"><%= item["verdict"] %></span></td></tr>
							<% i = i + 1 %>
							<% end -%>
						</tbody>
					</table>
					<%= will_paginate @resp, style: "margin-left: 15px;" %>
				</div>
			<% else %>
				<p> No submissions </p>
			<% end %>
		</div><!-- /.table-responsive -->
	</div>
</div>